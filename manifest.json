{
    "manifest_version": 3,
    "name": "Yume Squad",
    "description": "Soyez au courant des lives Twitch",
    "version": "1.0",
    "icons": {
        "128" : "img/logo.png"
    },
    "background": {
        "service_worker": "background.js"
    },

    "permissions": [ 
        "tabs",
        "alarms", 
        "notifications"
    ],
    "action" : {
        "default_popup" : "popup.html"
    }


    
}